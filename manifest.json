{
    "manifest_version": 2,
    "version": "1.0.0",
    "name": "Session Splitter Authenticated Proxy",
    "description": "This extension alters browser ip.",
    "permissions": [
      "activeTab", 
      "declarativeContent", 
      "storage",       
      "cookies",
      "privacy",   
      "webRequest",
      "webNavigation",
      "webRequestBlocking",      
      "<all_urls>",
      "proxy",
      "http://*/",
      "https://*/"],
  
    "icons": {
      "16": "images/16.png",
      "32": "images/32.png",
      "48": "images/48.png",
      "64": "images/64.png",
      "128": "images/128.png",
      "256": "images/256.png"
    },
  
    "browser_action": {
      "default_popup": "html/popup.html",
      "default_icon": {
        "16": "images/16.png",
        "32": "images/32.png",
        "48": "images/48.png",    
        "128": "images/128.png"
      }
    },
  
    "background": {
      "persistent": true,
      "scripts": [              
        "js/background.js"
      ]
    }

  }
  